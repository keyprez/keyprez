{
  "name": "keyprez",
  "version": "0.0.2",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/keyprez/keyprez.git"
  },
  "scripts": {
    "build": "svelte-kit build",
    "dev": "svelte-kit dev --open --port 8080",
    "format": "eslint --fix src/",
    "lint": "eslint src/",
    "pre-commit": "lint-staged",
    "prepare": "husky install",
    "preview": "svelte-kit preview",
    "start": "npm run dev"
  },
  "devDependencies": {
    "@sveltejs/adapter-netlify": "^1.0.0-next.35",
    "@sveltejs/kit": "^1.0.0-next.192",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^4.33.0",
    "autoprefixer": "^10.4.0",
    "eslint": "^8.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^3.2.1",
    "husky": "^7.0.4",
    "lint-staged": "^11.2.6",
    "postcss": "^8.3.11",
    "prettier": "~2.4.1",
    "prettier-plugin-svelte": "^2.4.0",
    "sass": "^1.43.4",
    "stream": "^0.0.2",
    "stylelint": "^13.13.1",
    "stylelint-config-sass-guidelines": "^8.0.0",
    "stylelint-config-standard": "^22.0.0",
    "svelte": "^3.44.0",
    "svelte-preprocess": "^4.9.8",
    "tslib": "^2.3.1",
    "typescript": "^4.4.4"
  },
  "lint-staged": {
    "*.{js,ts,svelte}": [
      "eslint --quiet"
    ],
    "*.scss": [
      "stylelint --syntax=scss"
    ]
  },
  "type": "module",
  "engines": {
    "node": ">14.0"
  },
  "dependencies": {
    "svelte-seo": "^1.3.1"
  }
}

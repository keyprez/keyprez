{
  "name": "keyprez",
  "version": "0.0.2",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/keyprez/keyprez.git"
  },
  "scripts": {
    "start": "npm run dev",
    "dev": "svelte-kit dev --open --port 8080",
